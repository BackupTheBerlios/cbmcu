<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="mcu" />
		<Option pch_mode="2" />
		<Option compiler="mingw" />
		<Build>
			<Target title="default">
				<Option output="$(#cb)\devel\share\CodeBlocks\plugins\mcu.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output="$(#cb)\.objs\plugins\contrib\mcu" />
				<Option type="3" />
				<Option compiler="mingw" />
				<Option parameters="--debug-log --no-check-associations -nd /p=mcu" />
				<Option host_application="$(#cb)\devel\codeblocks.exe" />
				<Compiler>
					<Add option="-g" />
					<Add option="-pipe" />
					<Add option="-mthreads" />
					<Add option="-fmessage-length=0" />
					<Add option="-fexceptions" />
					<Add option="-Winvalid-pch" />
					<Add option="-DBUILDING_PLUGIN" />
					<Add option="-DHAVE_W32API_H" />
					<Add option="-D__WXMSW__" />
					<Add option="-DWXUSINGDLL" />
					<Add option="-DcbDEBUG" />
					<Add option="-DCB_PRECOMP" />
					<Add option="-DWX_PRECOMP" />
					<Add option="-DwxUSE_UNICODE" />
					<Add option="-DWXMAKINGDLL_PROPGRID" />
					<Add option="-DwxPG_USE_WXMODULE=0" />
					<Add directory="$(#cb)\include" />
					<Add directory="$(#cb)\sdk\wxscintilla\include" />
					<Add directory="$(#wx.include)" />
					<Add directory="$(#wx.lib)\gcc_dll\mswu" />
					<Add directory="$(#cb)\devel" />
					<Add directory="..\mcumgr" />
				</Compiler>
				<Linker>
					<Add option="-Wl,--enable-auto-image-base" />
					<Add option="-Wl,--enable-auto-import" />
					<Add library="codeblocks" />
					<Add library="wxmsw28u" />
					<Add library="mcumgr.dll" />
					<Add directory="$(#cb)\devel" />
					<Add directory="$(#wx.lib)\gcc_dll" />
				</Linker>
				<ExtraCommands>
					<Add after="$(#zip)\zip -j9 $(#cb)\devel\share\CodeBlocks\mcu.zip resources\manifest.xml resources\*.xrc" />
					<Add after="$(#zip)\zip -rp $(#cb)\devel\share\CodeBlocks\mcu.zip images\*.*" />
				</ExtraCommands>
			</Target>
		</Build>
		<Linker>
			<Add directory="$(#cb)" />
		</Linker>
		<Unit filename="MicroTreeInfo.cpp" />
		<Unit filename="MicroTreeInfo.h" />
		<Unit filename="mainpanel.cpp" />
		<Unit filename="mainpanel.h" />
		<Unit filename="mcumain.cpp" />
		<Unit filename="mcumain.h" />
		<Unit filename="resources\manifest.xml" />
		<Unit filename="resources\mcu_toolbar.xrc" />
		<Unit filename="resources\mcu_toolbar_16x16.xrc" />
		<Unit filename="wxsmith\mainpanel.wxs" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<wxsmith version="1">
				<gui name="wxWidgets" src="" main="" init_handlers="necessary" language="CPP" />
				<resources>
					<wxPanel wxs="wxsmith/McuPanel.wxs" src="McuPanel.cpp" hdr="McuPanel.h" xrc="McuPanel.xrc" name="McuPanel" language="CPP" />
					<wxPanel wxs="wxsmith/mainpanel.wxs" src="mainpanel.cpp" hdr="mainpanel.h" xrc="resources/mainpanel.xrc" name="mainpanel" language="CPP" />
				</resources>
			</wxsmith>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
